[pull]
	rebase = true
[user]
	name = Christopher LUKOMBO
[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
	lol = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	ll = log --oneline
	feap = fetch --all --prune --verbose
	rc = rebase --continue
        ri = rebase -i
	unstage = reset HEAD
